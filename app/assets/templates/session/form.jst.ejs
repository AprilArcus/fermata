<div class="modal-content">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">
      <span aria-hidden="true">&times;</span>
      <span class="sr-only">Close</span>
    </button>
    <h4 class="modal-title" id="new-session-label">Sign In</h4>
  </div> <!-- /.modal-header -->
  <form role="form" id="session-form">
    <div class="modal-body">
      <div class="form-group">
        <label for="new-session-email">Email address</label>
        <input type="email"
               class="form-control"
               id="new-session-email"
               name="email"
               placeholder="Enter email">
      </div> <!-- /.form-group -->
      <div class="form-group">
        <label for="new-session-password">Password</label>
        <input type="password"
               class="form-control"
               id="new-session-password"
               name="password"
               placeholder="Password">
      </div> <!-- /.form-group -->
    </div> <!-- /.modal-body -->
    <div class="modal-footer">
      <button type="button" class="btn btn-default" id="new-user">New Account</button>
      <button type="submit" class="btn btn-primary">Sign In</button>
    </div> <!-- /.modal-footer -->
  </form>
</div>